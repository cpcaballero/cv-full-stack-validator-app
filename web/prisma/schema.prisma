generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  fullName   String
  email      String
  phone      String
  skills     String
  experience String
  createdAt  DateTime @default(now())
}
